class Person:
    name=None
    age=None
    address= None

    def print_details(self):
        print(f"{self.name} age is {self.age} and from {self.address}")

people_obj=Person()
name=input("Enter the name :\n")
people_obj.name =name
age=int(input("Enter age:\n"))
people_obj.age= age
address=input("Enter the address:\n")
people_obj.address=address
people_obj.print_details()
